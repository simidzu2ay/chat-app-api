{
  "name": "some-chat-backend",
  "version": "0.0.1",
  "description": "",
  "author": "Simidzu Ay",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:default": "nest start --watch",
    "start:dev": "tsc-watch --onSuccess \"node dist/main.js\" --onFailure \"echo There was a problem with the build!\" -p tsconfig.json",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.9",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.9",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-fastify": "^8.4.4",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-core": "^3.6.7",
    "apollo-server-fastify": "^3.6.7",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^7.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-ws": "^5.7.0",
    "oracledb": "https://github.com/oracle/node-oracledb/releases/download/v5.3.0/oracledb-src-5.3.0.tgz",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typeorm": "0.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
